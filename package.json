{
  "name": "@devnotnull/isomorphic-ssr-sls-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cleanup": "rimraf build/",
    "test": "jest",
    "prestart": "rimraf dist",
    "prebuild": "rimraf dist",
    "start": "sls offline start --noPrependStageInUrl",
    "watch": "nodemon --watch './build' --exec 'sls offline start --noPrependStageInUrl --aws-profile s3local'",
    "watch:types": "nodemon --watch './src' --exec 'yarn types'",
    "devclient": "NODE_ENV=development TARGET=client webpack serve --mode development --inline --hot",
    "prodclient": "NODE_ENV=production TARGET=client webpack --watch",
    "build:client": "TARGET=client webpack",
    "build:client:prod": "TARGET=client NODE_ENV=production webpack",
    "build:serverless": "TARGET=serverless webpack",
    "build:serverless:prod": "NODE_ENV=production TARGET=serverless webpack",
    "build": "yarn build:client && yarn build:server",
    "build:dev": "yarn cleanup && NODE_ENV=development yarn build:client && yarn build:server",
    "watch:server": "TARGET=serverless webpack --watch",
    "build:prod": "yarn cleanup && NODE_ENV=production yarn build:client && NODE_ENV=production yarn build:serverless",
    "test:types": "tsc --noEmit",
    "hook:before:offline:start": "yarn build:client:prod",
    "build:style-typings": "node webpack/style-typings.js",
    "lint": "eslint ./src --ext .js --ext .ts --ext .tsx",
    "lint:fix": "eslint ./src --ext .js --ext .ts --ext .tsx --fix",
    "types": "tcm src",
    "devserver:": "cross-env NODE_ENV=development TARGET=client webpack serve --mode development --inline --hot",
    "prodserver": "cross-env NODE_ENV=production TARGET=client webpack serve --mode production",
    "dev": "yarn cleanup && yarn build:style-typings && NODE_ENV=development IS_LOCAL=TRUE NODE_RUNTIME_ENV=local NODE_ENV=development concurrently -r --kill-others \"yarn devserver\" \"TARGET=serverless webpack --watch\" \"wait-on ./build/server.js && node build/server.js\"",
    "prod": "yarn cleanup && yarn build:style-typings && yarn build:client:prod && NODE_ENV=production IS_LOCAL=TRUE NODE_RUNTIME_ENV=local NODE_ENV=production concurrently -r --kill-others \"yarn prodserver\" \"TARGET=serverless webpack --watch\" \"wait-on ./build/serverless.js && yarn start\"",
    "deploy:dev": "yarn build:dev && serverless deploy --verbose --stage development",
    "deploy:prod": "yarn build:prod && serverless deploy --verbose --stage production"
  },
